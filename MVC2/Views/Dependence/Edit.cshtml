<body >
    <h1 style="text-align:center">Edit Dependence</h1>
    <form style="text-align:center" action=" /Dependence/Update">
 
        <div style="margin:10px">
            <label>Name</label>
            <input id="Name" name="name" type="text" value="@Model.Name" />
        </div>
        <div style="margin:10px">

            <label>Relationship</label>
            <input id="Relationship" name="Relationship" type="text" value="@Model.Relationship" />
        </div>
        <div style="margin:10px">

            <label>Birthdate</label>
            <input id="Birthdate" name="Birthdate" type="text" value="@Model.Birthdate" />
        </div>
        <div style="margin:10px">
            <a href="/Dependence/Delete/@Model.Name">Delete </a>
            <input type="submit" value="Update" />
        </div>
    </form>
